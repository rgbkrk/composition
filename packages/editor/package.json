{
  "name": "@nteract/editor",
  "version": "2.1.3",
  "description": "The editor that lives inside cells in nteract",
  "main": "lib/index.js",
  "nteractDesktop": "src/index.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run build:clean && npm run build:lib && npm run build:flow",
    "build:clean": "rimraf lib",
    "build:flow": "flow-copy-source -v -i '**/__tests__/**' src lib",
    "build:lib": "babel -d lib src --ignore '**/__tests__/**'",
    "build:lib:watch": "npm run build:lib -- --watch"
  },
  "keywords": [
    "nteract",
    "editor",
    "notebook",
    "jupyter"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@nteract/messaging": "^1.1.1",
    "@nteract/transforms": "^2.1.3",
    "classnames": "^2.2.5",
    "codemirror": "^5.27.0",
    "exenv": "^1.2.2",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10",
    "rxjs": "^5.4.1"
  },
  "peerDependencies": {
    "immutable": "^3.8.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "immutable": "^3.8.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "author": "Kyle Kelley <rgbkrk@gmail.com>",
  "license": "BSD-3-Clause"
}
